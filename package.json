{
	"name": "usher-js",
	"version": "0.0.5",
	"description": "Usher Satellite JS Library used for Conversion Tracking",
	"author": "Ryan Soury <ryan@usher.so>",
	"license": "MIT",
	"private": false,
	"main": "lib/index.cjs.js",
	"browser": "lib/index.js",
	"module": "lib/index.esm.js",
	"files": [
		"/lib"
	],
	"scripts": {
		"start": "cross-env NODE_ENV=development rollup -c",
		"dev": "cross-env NODE_ENV=development rollup -c -w",
		"build": "cross-env NODE_ENV=production rollup -c",
		"format": "prettier \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --write",
		"lint": "eslint -c ./.eslintrc.js \"src/**/*.{js,ts}\"",
		"test": "echo \"No tests available\"",
		"clean": "del ./lib",
		"local": "symlink ./lib ./tools/lib && serve -d ./tools -p 3001"
	},
	"dependencies": {
		"@babel/runtime": "^7.17.7",
		"eventemitter3": "^4.0.7",
		"framebus": "^5.1.2",
		"ky": "^0.30.0"
	},
	"devDependencies": {
		"@babel/core": "^7.17.7",
		"@babel/plugin-transform-runtime": "^7.17.0",
		"@babel/preset-env": "^7.16.11",
		"@gedhean/rollup-plugin-dotenv": "^0.3.1",
		"@rollup/plugin-alias": "^3.1.9",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-commonjs": "^21.0.2",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"cross-env": "^7.0.3",
		"del-cli": "^4.0.1",
		"dotenv": "^16.0.0",
		"eslint": "^8.11.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"np": "^7.6.1",
		"rollup": "^2.70.1",
		"rollup-plugin-filesize": "^9.1.2",
		"rollup-plugin-inject-process-env": "^1.3.1",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-sourcemaps": "^0.6.3",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-uglify": "^6.0.4",
		"rollup-plugin-visualizer": "^5.6.0",
		"symlink-cli": "^1.0.0"
	}
}
