{
	"name": "@usher.so/satellite",
	"version": "0.1.0",
	"description": "Usher Satellite Library used for integrating Conversion Tracking",
	"author": "Ryan Soury <ryan@usher.so>",
	"homepage": "https://usher.so/",
	"license": "MIT",
	"private": false,
	"main": "build/index.cjs.js",
	"browser": "build/index.js",
	"module": "build/index.esm.js",
	"files": [
		"/build"
	],
	"types": "index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/usherlabs/satellite.git"
	},
	"scripts": {
		"start": "cross-env NODE_ENV=development rollup -c",
		"dev": "cross-env NODE_ENV=development rollup -c -w",
		"build": "cross-env NODE_ENV=production rollup -c",
		"compile": "tsc --project ./tsconfig.build.json && resolve-tspaths",
		"format": "prettier \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --write",
		"lint": "eslint -c ./.eslintrc \"src/**/*.{js,ts}\"",
		"ts": "tsc --noEmit",
		"test": "echo \"No tests available\"",
		"clean": "del ./build",
		"local": "symlink ./build ./tools/build && serve -d ./tools -p 3001"
	},
	"dependencies": {
		"@aws-crypto/sha256-js": "^2.0.1",
		"@stablelib/random": "^1.0.1",
		"cuid": "^2.1.8",
		"dids": "^3.2.0",
		"eventemitter3": "^4.0.7",
		"framebus": "^5.1.2",
		"js-base64": "^3.7.2",
		"key-did-provider-ed25519": "^2.0.0",
		"key-did-resolver": "^2.0.4",
		"ky": "^0.30.0",
		"uint8arrays": "^3.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.17.7",
		"@babel/plugin-transform-runtime": "^7.17.0",
		"@babel/preset-env": "^7.16.11",
		"@gedhean/rollup-plugin-dotenv": "^0.3.1",
		"@rollup/plugin-alias": "^3.1.9",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-commonjs": "^21.0.2",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@typescript-eslint/eslint-plugin": "^5.30.3",
		"@typescript-eslint/parser": "^5.30.3",
		"cross-env": "^7.0.3",
		"del-cli": "^4.0.1",
		"dotenv": "^16.0.0",
		"esbuild": "^0.14.48",
		"eslint": "^8.19.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-typescript": "^3.1.5",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-unused-imports": "^2.0.0",
		"np": "^7.6.1",
		"prettier": "^2.7.1",
		"resolve-tspaths": "^0.6.0",
		"rollup": "^2.70.1",
		"rollup-plugin-esbuild": "^4.9.1",
		"rollup-plugin-filesize": "^9.1.2",
		"rollup-plugin-inject-process-env": "^1.3.1",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-polyfill-node": "^0.9.0",
		"rollup-plugin-sourcemaps": "^0.6.3",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-uglify": "^6.0.4",
		"rollup-plugin-visualizer": "^5.6.0",
		"symlink-cli": "^1.0.0",
		"tsc": "^2.0.4",
		"typescript": "^4.7.4"
	}
}
